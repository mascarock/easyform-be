{
  "version": 2,
  "buildCommand": "npm run build",
  "routes": [
    {
      "src": "/favicon.ico",
      "dest": "/public/favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ],
  "functions": {
    "api/index.js": {
      "includeFiles": "dist/**",
      "runtime": "nodejs20.x"
    }
  }
}
